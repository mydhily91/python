   "source": [
    "# write a programm to print the sum of digits in a number\n",
    "n = int(input(\"ENTER THE NUMBER  = \"))\n",
    "number = n\n",
    "sum = 0\n",
    "t = n\n",
    "while (t != 0):\n",
    "    remainder = t % 10\n",
    "    sum       = sum + remainder\n",
    "    t         = t / 10\n",
    "print(\"THE SUM NUMBER OF DIGITS IN %d is %d\"%(number,sum))"
   ]
